#%RAML 1.0 DataType
type: object
properties:
  recordLabel:
    type: string
    required: true
    example: "Outerscope"
  bands:
    type: array
    minItems: 1
    items:
      properties:
        name:
          type: string
          required: true
          example: "Winter Primates"
        Festival:
          type: string []
          example: ["LOL-palooza","LOL-palooza2"]


          




  